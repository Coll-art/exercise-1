/* ~~~~~~~~~~~ Exercise 1: Simple Array Operations ~~~~~~~~~~~ */
//step one
int[] numbers = [1, 2, 3, 4, 5];

for (int i = 0; i < numbers.Length; i++)
{
    Console.Write(numbers[i]);
}


//step 2
Console.WriteLine("give me 5 numbers");
int[] nums = [];

for (int i = 0; i < 5; i++)
{
    string givenNum = Console.ReadLine();
    int givenNumber = int.Parse(givenNum);
    nums.Append(givenNumber);
    
}
 Console.WriteLine(nums);
