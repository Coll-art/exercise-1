namespace ConsoleApp1;
// VEHICLE

public class Vehicle
{
    //instances

    private int Year;

    private string Make;
 
    private string Model;

    public Vehicle(int year, string make, string model)//shouldbe private?
    {
        Year = year;

        Make = make;

        Model = model;
    }

    public string ToString()
    {
        return $"{Year} {Make} {Model}";

       
    }

}


